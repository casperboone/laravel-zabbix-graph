{
    "name": "casperboone/laravel-zabbix-graph",
    "description": "Get nice looking graphs of your Zabbix devices in Laravel.",
    "keywords": [
        "laravel-zabbix-graph"
    ],
    "homepage": "https://github.com/casperboone/laravel-zabbix-graph",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Boone",
            "email": "mail@casperboone.nl",
            "homepage": "https://casperboone.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "casperboone/zabbix-graph": "^1.0",
        "illuminate/support": "~5.1|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4.6",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "CasperBoone\\LaravelZabbixGraph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CasperBoone\\LaravelZabbixGraph\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
